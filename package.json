{
  "name": "real-estate-app",
  "homepage": "https://jagoqui.github.io/real-estate-app/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A real estate application built with React and Vite, following clean code best practices and hexagonal architecture.",
  "author": {
    "name": "Jaidiver Gómez Quintero",
    "email": "jagoqui.gomez@gmail.com"
  },
  "maintainers": [
    {
      "name": "Jaidiver Gómez Quintero",
      "email": "jagoqui.gomez@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagoqui/real-estate-app.git"
  },
  "bugs": {
    "url": "https://github.com/jagoqui/real-estate-app/issues"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "tailwindcss",
    "real-estate",
    "clean-code",
    "hexagonal-architecture"
  ],
  "publisher": "jagoqui",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "dev": "vite",
    "staging": "vite --mode staging",
    "build": "tsc -b && vite build",
    "deploy": "npm run build && gh-pages -d dist",
    "check-types": "tsc --noemit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:staged": "vitest --changed --run",
    "test:watch": "vitest",
    "test:single": "node scripts/vitest-single-coverage.js",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --report-unused-disable-directives --fix --max-warnings 0",
    "format": "prettier --write src --ignore-unknown",
    "markdownlint": "markdownlint-cli2 --fix 'documentation/**/*.md'",
    "sonar-scanner:local": "docker rm -f sonar-scanner-cli; node ./scripts/sonar-scanner.js -local",
    "sonarqube:image:build": "docker rm -f sonarqube; docker run -d --name sonarqube -e SONAR_ES_BOOTSTRAP_CHECKS_DISABLE=true -p 9000:9000 sonarqube:latest",
    "sonarqube:image:start": "docker start sonarqube",
    "sonarqube:image:stop": "docker stop sonarqube",
    "snyk:auth": "npx snyk auth",
    "snyk:dependencies": "node --env-file=environments/.env ./scripts/snyk.js",
    "snyk:static": "node --env-file=environments/.env ./scripts/snyk.js -static",
    "snyk:monitor": "node --env-file=environments/.env ./scripts/snyk.js -monitor",
    "snyk:runner": "npm run snyk:static; npm run snyk:dependencies; npm run snyk:monitor",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-oauth/google": "^0.12.2",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.23",
    "@tanstack/react-router-devtools": "^1.132.23",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ky": "^1.10.0",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.3.12",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-path": "^2.0.3",
    "eslint-plugin-paths": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.2.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "happy-dom": "^18.0.1",
    "husky": "^8.0.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.0",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "3.6.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.4"
  }
}
